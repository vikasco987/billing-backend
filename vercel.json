{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "DATABASE_URL": "@DATABASE_URL"
  },
  "build": {
    "env": {
      "DATABASE_URL": "@DATABASE_URL"
    },
    "cache": {
      "node_modules": false
    }
  },
  "routes": [
    { "src": "/(.*)", "dest": "/" }
  ]
}
